{
	"rules": {
		"strict": 0,
		"prettier/prettier": "warn"
	},
	"env": {
		"node": true,
		"es6": true,
		"es2020": true
	},
	"extends": [
		"eslint:recommended",
		"plugin:import/recommended",
		"prettier"
	],
	"parserOptions": {
		"ecmaVersion": 2020,
		"sourceType": "module"
	},
	"plugins": [
		"prettier"
	]
}
